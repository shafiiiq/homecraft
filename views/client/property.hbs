        // generate unique key 
        function generateRandomKeyWithDate() {
            var chars = 'abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789!@#$%^&*()_+{}[]|\\;:\'",.<>?';
            var key = '';

            var currentDate = new Date();
            var month = currentDate.getMonth() + 1;
            var year = currentDate.getFullYear() % 100;

            var monthAbbreviations = ['JAN', 'FEB', 'MAR', 'APR', 'MAY', 'JUN', 'JUL', 'AUG', 'SEP', 'OCT', 'NOV', 'DEC'];
            var monthPrefix = monthAbbreviations[month - 1];

            var uniqueSegment = '';
            while (uniqueSegment.length < 6) {
                var randomIndex = Math.floor(Math.random() * chars.length);
                uniqueSegment += chars.charAt(randomIndex);
            }

            key = monthPrefix + year + uniqueSegment;

            return key;
        }


