        function displayFileName() {
            const fileInput = document.getElementById('fileInput');
            const fileNameSpan = document.getElementById('fileName');

            if (fileInput.files.length > 0) {
                fileNameSpan.textContent = fileInput.files[0].name;
            } else {
                fileNameSpan.textContent = "";
            }

            document.querySelector('.add-more').style.display = "flex"
        }


        function displayFileName1() {
            const fileInput = document.getElementById('fileInput-1');
            const fileNameSpan = document.getElementById('fileName-1');

            if (fileInput.files.length > 0) {
                fileNameSpan.textContent = fileInput.files[0].name;
            } else {
                fileNameSpan.textContent = "";
            }
        }

        function displayFileName2() {
            const fileInput = document.getElementById('fileInput-2');
            const fileNameSpan = document.getElementById('fileName-2');

            // Check if any file is selected
            if (fileInput.files.length > 0) {
                // Update the text content of the span with the selected file name
                fileNameSpan.textContent = fileInput.files[0].name;
            } else {
                // If no file is selected, clear the text content of the span
                fileNameSpan.textContent = "";
            }
        }

        function myProperty() {
            document.getElementById('my-property').style.backgroundColor = '#101010';
            document.getElementById('my-property').style.color = 'rgb(169 188 247)';
            document.getElementById('add').style.backgroundColor = 'rgb(0, 20, 87)';
            document.getElementById('add').style.color = '#ffffff';
        }

        function addProperty() {
            document.getElementById('my-property').style.backgroundColor = 'rgb(0, 20, 87)';
            document.getElementById('my-property').style.color = '#ffffff';
            document.getElementById('add').style.backgroundColor = '#101010';
            document.getElementById('add').style.color = 'rgb(169 188 247)';
        }

        document.getElementById('my-property').addEventListener('click', function () {
            document.querySelector('.my-properties').style.display = 'block';
            document.querySelector('.add-properties').style.display = 'none';
            document.querySelector('.all-properties').style.display = 'none';
            document.querySelector('.favourites').style.display = 'none';
            document.querySelector('.inbox').style.display = 'none';
            document.querySelector('.reviews').style.display = 'none';
        });

        document.getElementById('add').addEventListener('click', function () {
            document.querySelector('.add-properties').style.display = 'block';
            document.querySelector('.my-properties').style.display = 'none';
            document.querySelector('.all-properties').style.display = 'none';
            document.querySelector('.favourites').style.display = 'none';
            document.querySelector('.inbox').style.display = 'none';
            document.querySelector('.reviews').style.display = 'none';
        });

        document.getElementById('all').addEventListener('click', function () {
            document.querySelector('.all-properties').style.display = 'block';
            document.querySelector('.add-properties').style.display = 'none';
            document.querySelector('.my-properties').style.display = 'none';
            document.querySelector('.favourites').style.display = 'none';
            document.querySelector('.inbox').style.display = 'none';
            document.querySelector('.reviews').style.display = 'none';
            const items = document.querySelectorAll('.all-body .items');

            function getRandomDelay() {
                return Math.random() * 1000;
            }

            function isInViewport(element) {
                const rect = element.getBoundingClientRect();
                return (
                    rect.top >= 0 &&
                    rect.left >= 0 &&
                    rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
                    rect.right <= (window.innerWidth || document.documentElement.clientWidth)
                );
            }

            function handleScroll() {
                items.forEach((item, index) => {
                    if (isInViewport(item)) {
                        const delay = getRandomDelay();
                        setTimeout(() => {
                            item.classList.add('zoom-up');
                        }, delay);
                    }
                });
            }

            handleScroll();

            document.querySelector('.all-body').addEventListener('scroll', handleScroll);
        });

        document.getElementById('favourites').addEventListener('click', function () {
            document.querySelector('.favourites').style.display = 'block';
            document.querySelector('.add-properties').style.display = 'none';
            document.querySelector('.my-properties').style.display = 'none';
            document.querySelector('.all-properties').style.display = 'none';
            document.querySelector('.inbox').style.display = 'none';
            document.querySelector('.reviews').style.display = 'none';

            // favourites animations 
            const items = document.querySelectorAll('.fav-body .favs');

            function getRandomDelay() {
                return Math.random() * 1000;
            }

            function isInViewport(element) {
                const rect = element.getBoundingClientRect();
                return (
                    rect.top >= 0 &&
                    rect.left >= 0 &&
                    rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
                    rect.right <= (window.innerWidth || document.documentElement.clientWidth)
                );
            }

            function handleScroll() {
                items.forEach((item, index) => {
                    if (isInViewport(item)) {
                        const delay = getRandomDelay();
                        setTimeout(() => {
                            item.classList.add('favs-zoom-up');
                        }, delay);
                    }
                });
            }

            handleScroll();

            document.querySelector('.all-body').addEventListener('scroll', handleScroll);
        });

        document.getElementById('reviews').addEventListener('click', function () {
            document.querySelector('.reviews').style.display = 'block';
            document.querySelector('.favourites').style.display = 'none';
            document.querySelector('.add-properties').style.display = 'none';
            document.querySelector('.my-properties').style.display = 'none';
            document.querySelector('.all-properties').style.display = 'none';
            document.querySelector('.inbox').style.display = 'none';
        });

        document.getElementById('inbox').addEventListener('click', function () {
            document.querySelector('.inbox').style.display = 'block';
            document.querySelector('.favourites').style.display = 'none';
            document.querySelector('.add-properties').style.display = 'none';
            document.querySelector('.my-properties').style.display = 'none';
            document.querySelector('.all-properties').style.display = 'none';
            document.querySelector('.reviews').style.display = 'none';
        });

        document.querySelectorAll('.unlike').forEach(function (unlike) {
            unlike.addEventListener('click', function () {
                var liked = this.nextElementSibling;
                if (this.style.display == 'flex') {
                    liked.style.display = 'flex';
                    this.style.display = 'none';
                } else {
                    this.style.display = 'flex';
                    liked.style.display = 'none';
                }
            });
        });


        document.querySelectorAll('.liked').forEach(function (liked) {
            liked.addEventListener('click', function () {
                var unlike = this.previousElementSibling;
                if (this.style.display == 'flex') {
                    unlike.style.display = 'flex';
                    this.style.display = 'none';
                } else {
                    this.style.display = 'flex';
                    unlike.style.display = 'none';
                }
            });
        });


        // expand all propeties 
        function expandProperties(propertyTitle, propertyDescription, category, listedIn, price, yearlyTaxRate, landSize, bedrooms, bathrooms, kitchens, garages, garageSize, yearBuild, noFloors, about, acHeating, swimmingPool, parking, lakeView, bathtub, homeTheater, fireplace, garden, walkInCloset, hardwoodFloors, graniteCountertops, highCeilings, deckOrPatio, centralHeating, securitySystem, stainlessSteelAppliances, address, country, state, city, mapLocation, email) {
            var forExpand = document.querySelector('.expanded-properties');
            forExpand.classList.add('expanded');
            forExpand.classList.remove('shrinked');

            document.getElementById('swimmingPool').style.display = (swimmingPool ? 'flex' : 'none');
            document.getElementById('airConditioning').style.display = (acHeating ? 'flex' : 'none');
            document.getElementById('bathtub').style.display = (bathtub ? 'flex' : 'none');
            document.getElementById('homeTheater').style.display = (homeTheater ? 'flex' : 'none');
            document.getElementById('fireplace').style.display = (fireplace ? 'flex' : 'none');
            document.getElementById('garden').style.display = (garden ? 'flex' : 'none');
            document.getElementById('garage').style.display = (garages ? 'flex' : 'none');
            document.getElementById('walkInCloset').style.display = (walkInCloset ? 'flex' : 'none');
            document.getElementById('hardwoodFloors').style.display = (hardwoodFloors ? 'flex' : 'none');
            document.getElementById('graniteCountertops').style.display = (graniteCountertops ? 'flex' : 'none');
            document.getElementById('stainlessSteelAppliances').style.display = (stainlessSteelAppliances ? 'flex' : 'none');
            document.getElementById('highCeilings').style.display = (highCeilings ? 'flex' : 'none');
            document.getElementById('deckOrPatio').style.display = (deckOrPatio ? 'flex' : 'none');
            document.getElementById('centralHeating').style.display = (centralHeating ? 'flex' : 'none');
            document.getElementById('securitySystem').style.display = (securitySystem ? 'flex' : 'none');

            document.getElementById('country').innerHTML = `${country}`;
            document.getElementById('state').innerHTML = `${state}`;
            document.getElementById('city').innerHTML = `${city}`;
            document.getElementById('address').innerHTML = `${address}`;
            document.getElementById('location').innerHTML = `${mapLocation}`;
            document.getElementById('category').innerHTML = `${category}`;
            document.getElementById('yearsBuilt').innerHTML = `${yearBuild}`;
            document.getElementById('volume').innerHTML = `${landSize} sq.ft`;
            document.getElementById('floors').innerHTML = `${noFloors}`;
            document.getElementById('bedrooms').innerHTML = `${bedrooms}`;
            document.getElementById('bathrooms').innerHTML = `${bathrooms}`;
            document.getElementById('kitchens').innerHTML = `${kitchens}`;
            document.getElementById('description').innerHTML = `${propertyDescription}`;
            document.getElementById('expanded-property-name').innerHTML = `${propertyTitle}`;

            var imageElement = document.querySelector('.viewed-img').src = `/property-data/${email}landReciept.jpg`
        }

        function shrinkProperties() {
            var forExpand = document.querySelector('.expanded-properties');
            forExpand.classList.add('shrinked');
            forExpand.classList.remove('expanded');
