        //search facility
        const searchInput = document.getElementById('searchInput');
        const list = document.getElementById('animated-list');
        const listItems = list.querySelectorAll('.animated-list > li');

        // Function to filter list items based on search input
        function filterList() {
            const searchText = searchInput.value.toLowerCase();
            for (let i = 0; i < listItems.length; i++) {
                const listItemText = listItems[i].textContent.toLowerCase();
                if (listItemText.includes(searchText)) {
                    listItems[i].style.display = 'flex';
                } else {
                    listItems[i].style.display = 'none';
                }
            }
        }

        // Attach event listener to search input
        searchInput.addEventListener('input', filterList);

        // form changes activate the inputs
        document.getElementById('change').addEventListener('click', function () {
            var inputs = document.querySelectorAll('select');
            var submit = document.querySelector('.submit');
            inputs.forEach(function (input) {
