        // list animation 
        document.addEventListener("DOMContentLoaded", function () {
            const firstThreeItems = document.querySelectorAll('#animated-list .animation:nth-child(-n+3)');
            const otherItems = document.querySelectorAll('#animated-list .animation:not(:nth-child(-n+3))');
            const body = document.querySelector('.body');

            function isInViewport(element) {
                const rect = element.getBoundingClientRect();
                return (
                    rect.top >= 0 &&
                    rect.left >= 0 &&
                    rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
                    rect.right <= (window.innerWidth || document.documentElement.clientWidth)
                );
            }

            function handleScrollAnimation() {
                otherItems.forEach((item) => {
                    if (!item.classList.contains('animate') && isInViewport(item)) {
                        item.classList.add('animate');
                    }
                });
            }

            firstThreeItems.forEach((item, index) => {
                if (!item.classList.contains('animate')) {
                    item.style.animationDelay = `${index * 0.4}s`;
                    item.classList.add('animate');
                }
            });

            body.addEventListener('scroll', handleScrollAnimation);

            document.getElementById('contractors').addEventListener('click', function () {
                document.querySelector('.contractors').style.display = 'block';
            });

